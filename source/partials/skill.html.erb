<div class="row my-15">
  <div class="col-12 p-3">
    <h2>Skills</h2>
    <div class="break-line"></div>

    <div class="d-flex justify-content-between">
      <%# Languages %>
      <div class="skill-box text-center m-3">
        <i class="devicon-ruby-plain colored skill-icon"></i><p class="skill-text">Ruby</p>
      </div>

      <div class="skill-box text-center m-3">
        <i class="devicon-javascript-plain colored skill-icon"></i><p class="skill-text">JavaScript</p>
      </div>

      <div class="skill-box text-center m-3">
        <i class="devicon-html5-plain colored skill-icon"></i><p class="skill-text">HTML</p>
      </div>

      <div class="skill-box text-center m-3">
        <i class="devicon-css3-plain colored skill-icon"></i><p class="skill-text">CSS/SCSS</p>
      </div>

      <div class="skill-box text-center m-3">
        <i class="devicon-azuresqldatabase-plain colored skill-icon"></i><p class="skill-text">SQL</p>
      </div>
      <%# Frameworks %>
      <div class="skill-box text-center m-3">
        <i class="devicon-rails-plain colored skill-icon"></i><p class="skill-text">Ruby on Rails</p>
      </div>

      <div class="skill-box text-center m-3">
        <i class="devicon-bootstrap-plain colored skill-icon"></i><p class="skill-text">Bootstrap</p>
      </div>
      <%# Database %>
      <div class="skill-box text-center m-3">
        <i class="devicon-postgresql-plain colored skill-icon"></i><p class="skill-text">PostgreSQL</p>
      </div>
      <%# Tools %>
      <div class="skill-box text-center m-3">
        <i class="devicon-heroku-plain colored skill-icon"></i><p class="skill-text">Heroku</p>
      </div>

      <div class="skill-box text-center m-3">
        <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/netlify/netlify-original.svg"><p class="skill-text">Netlify</p>
      </div>

       <div class="skill-box text-center m-3">
        <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/figma/figma-original.svg"><p class="skill-text">Figma</p>
      </div>

    </div>
  </div>
</div>
